            android:layout_width="320dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="80dp"
            android:background="#2B2B2B"
            app:iconifiedByDefault="false"
            app:queryHint="Search here ..." />

        <LinearLayout
